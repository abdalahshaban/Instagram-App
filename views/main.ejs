<% include ../views/partials/header%>
    <div class="grid">
    
        <div class='col_9'> 
            <h1><%=title%></h1>
            <%medias.forEach((media)=>{%>
            <div class="col_4">        
                <a href="#">
                    <img src="<%=media.images.low_resolution.url%>" alt="">
                    <small class="meta">Posted by <%=media.user.username%> on <%= formateDate( media.created_time)%></small>
                </a>
              </div>
           <% })%>
         
        </div>
        <div class='col_3'>
            <h5>welcome, <br><%=user.username%></h5>
            <h5>welcome, <br><%=user.full_name%></h5>
            <div class='redbox'>
                <h5>Post</h5>
                <h4><%=user.counts.media%></h4>
            </div>
            <div class='bluebox'>
                <h5>followers</h5>
                <h4><%=user.counts.followed_by%></h4>
            </div>
            <div class='greenbox'>
                <h5>follwers</h5>
                <h4><%=user.counts.follows%></h4>
            </div>
        </div>

    </div>
 <% include ../views/partials/footer%>
